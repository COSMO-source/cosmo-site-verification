{
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/.well-known/(.*)",
      "dest": "/.well-known/$1"
    },
    {
      "src": "/privacy/(.*)",
      "dest": "/privacy/$1"
    },
    {
      "src": "/terms/(.*)",
      "dest": "/terms/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
